INPUT
T_FCGIOBJNOMUNGE
    if (sv_isa($arg, \"FCGI\")) {
	$var = ($type) SvIV((SV*)SvRV($arg));
    } else
	croak(\"$var is not of type ${ntype}\")
T_FCGIREQUEST
